public struct Number 
{
    public let x: float32;
}

public function newNumber(): Number
{}

public function newBoolFirst(): bool 
{}

public function newBoolSecond(): bool
{}

public function main(): void
{
    // basic binary
    0 + 1f32;
    2 >= -3;
    true == true;

    // assignment with mutation expected
    let number = newNumber();
    number.x = 4;

    // logical short circuit
    newBoolFirst() and newBoolSecond();
}