extern "C":
    public function printf(fmt: &uint8, ...) -> int32
    public function scanf(fmt: &uint8, ...) -> int32


public function main():
    let mut n = 0
    scanf(&"%d\0".[0], &mut n)

    let mut i = 1
    
    while i <= n:
        if i % 3 == 0 and i % 5 == 0:
            printf(&"FizzBuzz\n\0".[0])
        else if i % 3 == 0:
            printf(&"Fizz\n\0".[0])
        else if i % 5 == 0:
            printf(&"Buzz\n\0".[0])
        else:
            printf(&"%d\n\0".[0], i)
        
        i += 1