public enum Option[T]:
    Some(T)
    None


public function main():
    Option[int32]::Some()              # expected 1 value
    Option[int32]::Some(53, 42)        # expected 1 value
    Option[int32]::None()       # can't call
    Option[int32]::None(32)     # can't call
    Option[int32]::None(32, 64) # can't call
    Option::Some(32)
