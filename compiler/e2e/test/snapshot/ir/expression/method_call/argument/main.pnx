public struct Pair[T, U]:
    public first: T
    public second: U


implements Pair[int32, float32]:
    public function add(self: &mut this, first: int32, second: float32):
        pass


public function main():
    let mut p = Pair { first: 32, second: 64.0 }
    p.add(1, 2.0)
    p.add(1, 2)          # expect error
    p.add(1.0, 2.0)      # expect error
    p.add(true, false)   # expect error
    
    p.add() # count error
    p.add(1) # count error
    p.add(1, 2.0, 3) # count error
        