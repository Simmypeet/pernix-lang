public enum Option<T> {
    Some(T),
    None
}

public trait Pair<T> {
    type Output<V: T>;
}

implements Pair<int32>
{
    type Output<V: Test<Option<int32>>::A> = float32;
}

public trait Test<T> {
    type A;
}

implements<T> Test<T> {
    type A = T;
}

implements<T> Test<Option<T>> {
    type A = T;
}
